{
    "name": "webpack-demo",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env BUILD_ENV=dev webpack --mode development --watch --config ./build/webpack.config.mini.js",
        "test": "cross-env BUILD_ENV=test webpack --mode development --watch --config ./build/webpack.config.mini.js",
        "bate": "cross-env BUILD_ENV=bate webpack --mode development --watch --config ./build/webpack.config.mini.js",
        "build": "cross-env BUILD_ENV=prod webpack --mode production --config ./build/webpack.config.mini.js",
        "prod": "cross-env BUILD_ENV=prod webpack --mode production  --watch --config ./build/webpack.config.mini.js",
        "dev:api": "node node_modules/swagger-to-ts-schema url=./build/bin/swagger-dev.json output=./src/api force",
        "dev:api-valid": "node node_modules/swagger-to-ts-schema url=./build/bin/swagger-dev.json output=./src/api force type=valid scopeDir=./src/api",
        "debuger": "node --inspect-brk ./node_modules/webpack/bin/webpack.js --inline --progress"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.11.0",
        "@babel/runtime": "^7.5.1",
        "autoprefixer": "^8.5.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^8.1.0",
        "cache-loader": "^1.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^7.0.2",
        "eslint": "^5.0.1",
        "eslint-config-airbnb": "^16.1.0",
        "file-loader": "^1.1.11",
        "file-save": "^0.2.0",
        "fs-extra": "^4.0.2",
        "htmlstr-parser": "^2.0.1",
        "json-loader": "^0.5.7",
        "less": "^3.12.2",
        "less-loader": "^7.0.2",
        "mini-program-webpack-loader": "^1.0.24",
        "miniprogram-api-typings": "*",
        "node-file-eval": "^1.0.0",
        "pkg-dir": "^5.0.0",
        "rimraf": "^2.6.2",
        "string-replace-loader": "^2.3.0",
        "style-resources-loader": "^1.3.3",
